<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qust.zq.carcare"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name="com.qust.zq.carcare.GlobalApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.qust.zq.carcare.activity.MainActivity"
            android:label="@string/activity_login" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.qust.zq.carcare.activity.MyCarListActivity"
            android:label="@string/activity_mycar" >
        </activity>
        <activity
            android:name="com.qust.zq.carcare.activity.CarSelectActivity"
            android:label="@string/activity_select" >
        </activity>
        <activity
            android:name="com.qust.zq.carcare.activity.CarCareActivity"
            android:label="@string/activity_carcare" >
        </activity>
    </application>

</manifest>